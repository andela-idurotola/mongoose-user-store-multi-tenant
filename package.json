{
  "author": "Martin Wawrusch <martin@wawrusch.com> (http://martinatsunset.com)",
  "name": "mongoose-user-store-multi-tenant",
  "description": "A bunch of mongoose schemas to implement identity management (users, accesstokens, oauth apps)",
  "version": "1.1.2",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/codedoctor/mongoose-user-store-multi-tenant.git"
  },
  "bin": {},
  "directories": {
    "lib": "./lib",
    "test": "./test"
  },
  "keywords": [
    "mongoose",
    "identity",
    "user",
    "oauth2"
  ],
  "scripts": {
    "test": "node_modules/.bin/grunt testandcoverage",
    "docs": "node_modules/.bin/codo -i ./src -o ./docs"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "async": "0.9.0",
    "boom": "2.6.1",
    "date-utils": "1.2.16",
    "hoek": "2.10.0",
    "mongoose-plugins-accessible-by": "0.2.2",
    "mongoose-plugins-created-by": "0.4.1",
    "mongoose-plugins-delete-paranoid": "0.2.0",
    "mongoose-plugins-resource-limits": "0.2.0",
    "mongoose-plugins-tags-simple": "0.2.0",
    "mongoose-plugins-timestamp": "0.2.0",
    "mongoose-rest-helper": "1.0.2",
    "passgen": "1.0.1",
    "underscore": "1.7.0",
    "bcryptjs": "2.0.2",
    "underscore-ext": "0.2.0",
    "mongodb-objectid-helper": "0.2.0"
  },
  "peerDependencies": {
    "mongoose": ">=3.8.14"
  },
  "devDependencies": {
    "blanket": "1.1.6",
    "codo": "2.0.9",
    "coffee-script": "1.8.0",
    "grunt": "0.4.5",
    "grunt-cli": "0.1.13",
    "grunt-codo": "0.2.0",
    "grunt-contrib-coffee": "0.12.0",
    "grunt-contrib-watch": "0.6.1",
    "grunt-env": "0.4.2",
    "grunt-mocha-cov": "0.3.0",
    "grunt-mocha-test": "0.12.4",
    "grunt-release": "0.9.0",
    "grunt-shell": "1.1.1",
    "matchdep": "0.3.0",
    "mocha": "2.0.1",
    "mongoose": "3.8.20",
    "mongoskin": "1.4.4",
    "should": "4.4.1"
  },
  "bugs": {
    "url": "http://github.com/codedoctor/mongoose-user-store-multi-tenant/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/codedoctor/mongoose-user-store-multi-tenant/raw/master/LICENSE"
    }
  ],
  "config": {
    "blanket": {
      "pattern": [
        "lib"
      ],
      "data-cover-never": "node_modules"
    }
  }
}
